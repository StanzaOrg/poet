defpackage poet/commands:
  import core

protected defn init (project-dir*: String|False) -> False:
  poet/commands/init/init(project-dir*)

protected defn add (kind: String, dependency: String) -> False:
  poet/commands/add/add(kind, dependency)

protected defn build (args: Tuple<String>) -> False:
  poet/commands/build/build(args)

protected defn clean () -> False:
  poet/commands/clean/clean()
